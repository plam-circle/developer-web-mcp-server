{
  "name": "developer-web-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Circle's developer-web project management and code analysis",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "fastmcp dev src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:env": "node -r dotenv/config dist/index.js",
    "inspect": "fastmcp inspect src/index.ts"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "fastmcp": "^3.17.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "mcp",
    "developer-web",
    "circle",
    "project-management"
  ],
  "author": "Circle Developer Team",
  "license": "MIT"
}
